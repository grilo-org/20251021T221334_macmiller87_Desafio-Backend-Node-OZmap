{
  "name": "Desafio-Backend-Node-OZmap",
  "version": "1.0.0",
  "main": "server.ts",
  "repository": "https://github.com/macmiller87/Desafio-Backend-Node-OZmap.git",
  "author": "macmiller87 <macamagolf@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --watch --env-file=.env src/server.ts",
    "dev:debug": "ts-node-dev --inspect --env-file=.env src/server.ts",
    "json:server:origindb": "npx json-server src/mock-database/db.json",
    "json:server:ozmap_output": "npx json-server src/utils/copy-mock-database/ozmap_output/ozmap_output.json",
    "dc:up": "docker compose up -d && docker compose logs -f mongodb_app",
    "dc:down": "docker compose down --volumes --remove-orphans",
    "setup:prisma": "npx prisma generate && npx prisma db push",
    "build": "tsc"
  },
  "devDependencies": {
    "@ozmap/ozmap-sdk": "^1.1.5",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@types/node-schedule": "^2.1.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "express": "^5.1.0",
    "json-server": "^1.0.0-beta.3",
    "node-schedule": "^2.1.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "winston": "^3.17.0"
  }
}
